<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Lecture" table="lecture">
    <indexes>
      <index name="iduser" columns="iduser"/>
      <index name="area" columns="area"/>
      <index name="conference" columns="conference_participant"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="text" column="title" length="65535" nullable="true"/>
    <field name="summary" type="text" column="summary" length="65535" nullable="true"/>
    <field name="recordDate" type="datetime" column="record_date" nullable="true"/>
    <field name="canceled" type="integer" column="canceled" nullable="false"/>
    <many-to-one field="area" target-entity="Areas">
      <join-columns>
        <join-column name="area" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="iduser" target-entity="User">
      <join-columns>
        <join-column name="iduser" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="conferenceParticipant" target-entity="ConferenceParticipant">
      <join-columns>
        <join-column name="conference_participant" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
