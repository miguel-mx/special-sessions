<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Lecture" table="lecture">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="iduser" type="integer" column="iduser" nullable="true"/>
    <field name="title" type="text" column="title" length="65535" nullable="true"/>
    <field name="area" type="integer" column="area" nullable="true"/>
    <field name="summary" type="text" column="summary" length="65535" nullable="true"/>
    <field name="conferenceParticipant" type="integer" column="conference_participant" nullable="true"/>
    <field name="recordDate" type="datetime" column="record_date" nullable="true"/>
    <field name="canceled" type="integer" column="canceled" nullable="false"/>
  </entity>
</doctrine-mapping>
